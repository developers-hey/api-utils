							"event": [
								{
									"listen": "prerequest",
									"script": {
										"exec": [
											"var date = new Date()\r",
											"var year=date.getFullYear();\r",
											"var month=date.getMonth()+1\r",
											"var firstDay = \"01\"\r",
											"var lastDay = date.getDate();\r",
											"\r",
											"if(lastDay < 10){\r",
											"    lastDay = \"0\"+lastDay;\r",
											"}\r",
											"\r",
											"var from=year+\"-\"+month+\"-\"+firstDay;\r",
											"var to=year+\"-\"+month+\"-\"+lastDay;\r",
											"\r",
											"pm.environment.set(\"from\",from)\r",
											"pm.environment.set(\"to\",to)"
										],
